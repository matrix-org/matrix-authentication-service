{
  "name": "mas-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "generate": "graphql-codegen && eslint --fix .",
    "lint": "graphql-codegen && eslint . && tsc",
    "build": "npm run lint && vite build --base=./ && npm run build:templates",
    "build:templates": "tailwindcss --postcss --minify --config ./tailwind.templates.config.cjs -o dist/tailwind.css",
    "preview": "vite preview",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@urql/core": "^3.2.2",
    "date-fns": "^2.29.3",
    "graphql": "^16.6.0",
    "jotai": "^2.0.3",
    "jotai-location": "^0.5.1",
    "jotai-urql": "^0.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^3.2.2",
    "@graphql-codegen/client-preset": "^2.1.1",
    "@graphql-eslint/eslint-plugin": "^3.16.1",
    "@storybook/addon-actions": "^7.0.0",
    "@storybook/addon-backgrounds": "^7.0.5",
    "@storybook/addon-controls": "^7.0.0",
    "@storybook/addon-docs": "^7.0.0",
    "@storybook/addon-essentials": "^7.0.0",
    "@storybook/addon-measure": "^7.0.0",
    "@storybook/addon-outline": "^7.0.0",
    "@storybook/addon-toolbars": "^7.0.0",
    "@storybook/addon-viewport": "^7.0.0",
    "@storybook/react": "^7.0.0",
    "@storybook/react-vite": "^7.0.0",
    "@types/node": "^18.15.5",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/react-test-renderer": "^18.0.0",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitest/coverage-c8": "^0.29.7",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-prettier": "^4.2.1",
    "postcss": "^8.4.21",
    "prettier": "^2.8.6",
    "react-test-renderer": "^18.2.0",
    "storybook": "^7.0.0",
    "tailwindcss": "^3.2.7",
    "typescript": "^5.0.2",
    "vite": "^4.2.1",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-graphql-codegen": "^3.1.0",
    "vitest": "^0.29.7"
  }
}
