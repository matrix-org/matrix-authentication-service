{
  "compilerOptions": {
      "target": "ES2022",
      "module": "ES2022",
      "lib": ["ES2022"],
      "moduleResolution": "node",
      "strict": true,
      "sourceMap": true,
      "declaration": false,
      "outDir": "./dist",
      "rootDir": "./src",
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "skipLibCheck": true
  }
}